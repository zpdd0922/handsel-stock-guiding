@import '~common/stylus/common/white-theme'
@import '~common/stylus/common/black-theme'
@import '~common/stylus/mixin'

.bg-wrap
  background-color $white-theme-eff0f0
.bg-wrap-black
  background-color $black-theme-1d1d1f
.data-wrap
  width 100%
  height 100%
  background-color $white-theme-fff
  .no-data
    & > div
      posCenter()
      color $white-theme-424242
.data-wrap-black
  background-color $black-theme-1d1d1f
  .no-data
    & > div
      posCenter()
      color $white-theme-fff
.container
  .gold
    background-color $white-theme-fff
  .gold-mt
    margin-top 20px
  .stock
    margin-top 20px
    background-color $white-theme-fff
  .waitGetStock
    background-color $white-theme-fff
    border-1px('bottom', $white-theme-e5e5e5)
    height 70px
    p
      height 70px
      line-height 70px
      font-size:30px;
      font-weight:500;
      color: $white-theme-333;
      padding-left 26px 
.container-black
  .gold, .stock
    background-color $black-theme-242426
  .waitGetStock
    background-color $black-theme-242426
    border-1px('bottom', $white-theme-2e2e33)
    p
      color: $black-theme-fff;
 
.modelBox
  position relative
  width 610px
  height 560px
  border-bottom-right-radius: 20px; 
  border-bottom-left-radius: 20px;
  background-color #fff
  .head
    width 610px
    height 207px
    padding-top 10px
    bg-image('home/rule-bg')
    background-size 100%
    .title
      height:80px;
      width: 130px;
      margin: 0 auto;
      text-align center;
      font-size:42px;
      font-weight:600;
      color:rgba(255,255,255,1);
      line-height:80px;
  .content 
    height 220px
    width 100%
    padding-top 34px
    text-align center
    .getStock
      height: 28px
      font-size: 28px  
      font-weight: 500
      color:#333;
      line-height: 28px
      margin: 0px auto 20px
    .stkName
      color #FD6D40
    .stkQuantity
      height:70px;
      font-size:70px;
      font-weight:500;
      color:rgba(253,109,64,1);
      line-height:70px;
      margin: 0 auto 26px
    .tips
      height:28px;
      font-size:28px;
      font-weight:500;
      color:rgba(51,51,51,1);
      line-height:28px;
      margin:0 auto
  .footer
    height: 80px  
    text-align center
    .with-click 
      color #999
      font-size: 22px
      padding 0 6px
      &>>>.cube-checkbox-wrap
        justify-content: center;
  .clickBtn
    position absolute
    left 80px
    width:460px;
    height:90px;
    background:#FFE25C;
    border-radius:45px;
    p
      height:90px;
      font-size:32px;
      font-weight:bold;
      color:rgba(181,92,38,1);
      line-height:90px;
      text-align center
  .closeBtn
    position absolute
    left 280px
    bottom -150px
    width:54px;
    height:54px;
    bg-image('home/close')
    background-size 100%
  
  
.cube-popup>>>.cube-popup-mask 
  opacity: 0.7;

