  "lint-staged": {
    "**/**.{js,json,pcss,md,vue}": [
      "prettier --write",
      "git add"
    ],
    "*.styl": [
      "stylelint --fix",
      "git add"
    ],
    "*.{png,jpeg,jpg,gif,svg}": [
      "imagemin-lint-staged",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit-msg && lint-staged"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
